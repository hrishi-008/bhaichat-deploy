{
  "builds": [
    {
      "src": "mychat/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mychat/wsgi.py"
    }
  ],
  "functions": {
    "installDjango": {
      "runtime": "python3.9",
      "handler": "installDjango.install",
      "includeFiles": ["installDjango.py", "djangoprojectname/**"],
      "memory": 512
    }
  }
}
